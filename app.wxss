/**app.wxss**/
.container {                        /*定义整体样式样式*/
  height: 100%;
  display: flex;
  flex-direction: column;           /*伸缩流方向为纵向*/
  align-items: center;              /*设置项目在其包含块中在交叉轴方向上的对齐方式。*/
  justify-content: space-between;   /*均匀排列每个元素*/
  padding: 200rpx 0;                /*内边距*/
  box-sizing: border-box;           /*边界线在容器内部*/
} 
